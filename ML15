      "source": [
        "import numpy as np\n",
        "\n",
        "class LogisticRegression:\n",
        "    def __init__(self, n_features):\n",
        "        # Initialize weights as a vector of zeros, and bias to zero\n",
        "        self.weights = np.zeros(n_features)\n",
        "        self.bias = 0\n",
        "        print(\"Logistic Regression model initialized with:\")\n",
        "        print(f\"  Weights shape: {self.weights.shape}\")\n",
        "        print(f\"  Bias: {self.bias}\")\n",
        "\n",
        "    def sigmoid(self, z):\n",
        "        # Compute the sigmoid activation function\n",
        "        return 1 / (1 + np.exp(-z))\n",
        "\n",
        "    def compute_loss(self, y, y_pred):\n",
        "        # Compute binary cross-entropy loss\n",
        "        # Add a small epsilon to avoid log(0) issues\n",
        "        epsilon = 1e-10\n",
        "        y_pred = np.clip(y_pred, epsilon, 1 - epsilon)\n",
        "        loss = -np.mean(y * np.log(y_pred) + (1 - y) * np.log(1 - y_pred))\n",
        "        return loss\n",
        "\n",
        "    def compute_gradients(self, X, y, y_pred):\n",
        "        # Compute gradients for weights (dw) and bias (db)\n",
        "        m = X.shape[0] # Number of samples\n",
        "\n",
        "        # Gradient of loss with respect to weights\n",
        "        dw = (1 / m) * np.dot(X.T, (y_pred - y))\n",
        "\n",
        "        # Gradient of loss with respect to bias\n",
        "        db = (1 / m) * np.sum(y_pred - y)\n",
        "\n",
        "        return dw, db\n",
        "\n",
        "print(\"LogisticRegression class defined successfully. You can now instantiate it.\")"
      ],
      "execution_count": 6,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "LogisticRegression class defined successfully. You can now instantiate it.\n"
          ]
        }
      ]
