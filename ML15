        "# d. Label the y-axis\n",
        "plt.ylabel('Loss')\n",
        "# Clearly distinguish with a legend\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "# e. Display the plot\n",
        "plt.show()\n",
        "\n",
