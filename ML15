      "source": [
        "import numpy as np\n",
        "from sklearn.metrics import accuracy_score, confusion_matrix, classification_report\n",
        "\n",
        "class LogisticRegression:\n",
        "    def __init__(self, n_features):\n",
        "        self.weights = np.zeros(n_features)\n",
        "        self.bias = 0\n",
        "        self.losses = [] # To store loss during training\n",
        "        # print(\"Logistic Regression model initialized with:\")\n",
        "        # print(f\"  Weights shape: {self.weights.shape}\")\n",
        "        # print(f\"  Bias: {self.bias}\")\n",
        "\n",
        "    def sigmoid(self, z):\n",
        "        return 1 / (1 + np.exp(-z))\n",
        "\n",
        "    def compute_loss(self, y, y_pred):\n",
        "        epsilon = 1e-10\n",
        "        y_pred = np.clip(y_pred, epsilon, 1 - epsilon)\n",
        "        loss = -np.mean(y * np.log(y_pred) + (1 - y) * np.log(1 - y_pred))\n",
        "        return loss\n",
        "\n",
        "    def compute_gradients(self, X, y, y_pred):\n",
        "        m = X.shape[0]\n",
        "        dw = (1 / m) * np.dot(X.T, (y_pred - y))\n",
        "        db = (1 / m) * np.sum(y_pred - y)\n",
        "        return dw, db\n",
        "\n",
        "    def fit(self, X, y, learning_rate, n_iterations):\n",
        "        # Ensure X and y are numpy arrays\n",
        "        X = X.to_numpy() if isinstance(X, pd.DataFrame) else X\n",
        "        y = y.to_numpy() if isinstance(y, pd.Series) else y\n",
        "\n",
        "        for i in range(n_iterations):\n",
        "            # Linear combination\n",
        "            z = np.dot(X, self.weights) + self.bias\n",
        "\n",
        "            # Predicted probabilities\n",
        "            y_pred = self.sigmoid(z)\n",
        "\n",
        "            # Compute and store loss\n",
        "            loss = self.compute_loss(y, y_pred)\n",
        "            self.losses.append(loss)\n",
        "\n",
        "            # Compute gradients\n",
        "            dw, db = self.compute_gradients(X, y, y_pred)\n",
        "\n",
        "            # Update weights and bias\n",
        "            self.weights -= learning_rate * dw\n",
        "            self.bias -= learning_rate * db\n",
        "\n",
        "            if (i + 1) % (n_iterations / 10) == 0: # Print progress roughly every 10% of iterations\n",
        "                print(f\"Iteration {i+1}/{n_iterations}, Loss: {loss:.4f}\")\n",
        "        print(\"Training complete.\")\n",
        "\n",
        "    def predict_proba(self, X):\n",
        "        # Ensure X is a numpy array\n",
        "        X = X.to_numpy() if isinstance(X, pd.DataFrame) else X\n",
        "        z = np.dot(X, self.weights) + self.bias\n",
        "        return self.sigmoid(z)\n",
        "\n",
        "    def predict(self, X, threshold=0.5):\n",
        "        probabilities = self.predict_proba(X)\n",
        "        return (probabilities >= threshold).astype(int)\n",
        "\n",
        "\n",
        "# --- Training and Evaluation ---\n",
        "\n",
        "# Convert DataFrames to NumPy arrays for compatibility\n",
        "X_train_np = X_train.to_numpy()\n",
        "X_test_np = X_test.to_numpy()\n",
        "y_train_np = y_train.to_numpy()\n",
        "y_test_np = y_test.to_numpy()\n",
        "\n",
        "# 1. Initialize the custom Logistic Regression model\n",
        "n_features = X_train_np.shape[1]\n",
        "model = LogisticRegression(n_features=n_features)\n",
        "\n",
        "# 2. Train the model\n",
        "learning_rate = 0.01\n",
        "n_iterations = 5000\n",
        "print(f\"\\nStarting training with learning rate: {learning_rate}, iterations: {n_iterations}\")\n",
        "model.fit(X_train_np, y_train_np, learning_rate=learning_rate, n_iterations=n_iterations)\n",
        "\n",
        "# 3. Make predictions on the unscaled X_test\n",
        "y_pred_test = model.predict(X_test_np)\n",
        "\n",
        "# 4. Evaluate the model's performance\n",
        "print(\"\\n--- Model Evaluation (without Scaling) ---\")\n",
        "print(f\"Accuracy: {accuracy_score(y_test_np, y_pred_test):.4f}\")\n",
        "print(\"\\nConfusion Matrix:\")\n",
        "print(confusion_matrix(y_test_np, y_pred_test))\n",
        "print(\"\\nClassification Report:\")\n",
        "print(classification_report(y_test_np, y_pred_test))\n"
      ],
      "execution_count": 7,
