      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# 1. Define a list of learning rates to experiment with\n",
        "learning_rates = [0.1, 0.01, 0.001]\n",
        "\n",
        "# 2. Initialize an empty dictionary to store losses for each learning rate\n",
        "losses_by_lr = {}\n",
        "\n",
        "n_iterations_experiment = 5000\n",
        "n_features_scaled = X_train_scaled.shape[1] # Use the number of features from scaled data\n",
        "\n",
        "print(\"\\n--- Experimenting with different learning rates ---\")\n",
        "\n",
        "# 3. Loop through each learning rate, train the model, and store losses\n",
        "for lr in learning_rates:\n",
        "    print(f\"\\nTraining with learning rate: {lr}\")\n",
        "    # a. Create a new instance of the LogisticRegression class\n",
        "    model_lr = LogisticRegression(n_features=n_features_scaled)\n",
        "\n",
        "    # b. Call the fit method\n",
        "    model_lr.fit(X_train_scaled, y_train_np, learning_rate=lr, n_iterations=n_iterations_experiment)\n",
        "\n",
        "    # c. Store the losses\n",
        "    losses_by_lr[lr] = model_lr.losses\n",
        "\n",
        "# 5. Create a plot to visualize the loss convergence\n",
        "plt.figure(figsize=(10, 6))\n",
        "for lr, losses in losses_by_lr.items():\n",
        "    plt.plot(range(len(losses)), losses, label=f'LR = {lr}')\n",
        "\n",
        "# b. Add a title\n",
        "plt.title('Loss Convergence for Different Learning Rates')\n",
        "# c. Label the x-axis\n",
